{
  "name": "template-node",
  "version": "1.0.0",
  "description": "Partners API",
  "main": "src/server.js",
  "license": "MIT",
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "src/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "server": "set DEBUG=express:* & node ./dist-src/server",
    "dev": "NODE_ENV=development npm-run-all build server",
    "prod": "NODE_ENV=production npm-run-all build server",
    "transpile": "babel ./src --out-dir dist-src",
    "clean": "rimraf dist-server",
    "watch:dev": "nodemon"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/transform-runtime"
      ]
    ]
  },
  "dependencies": {
    "-": "^0.0.1",
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/runtime": "^7.14.8",
    "@sentry/node": "^5.29.1",
    "aws-sdk": "^2.806.0",
    "axios": "^0.21.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^1.2.0",
    "g": "^2.0.1",
    "hashids": "^2.2.1",
    "image-size": "^0.9.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "mysql2": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "rimraf": "^3.0.2",
    "sequelize": "^6.11.0",
    "starkbank": "^2.8.0",
    "sucrase": "^3.10.1",
    "uuid": "^8.3.2",
    "uuidv4": "^6.2.12",
    "youch": "^2.0.10"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.12.10",
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.0",
    "nodemon": "^2.0.13",
    "prettier": "^1.18.2",
    "sequelize-cli": "^6.2.0",
    "sucrase": "^3.10.1"
  }
}
